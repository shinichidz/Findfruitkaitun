repeat wait() until game:IsLoaded() and game.Players.LocalPlayer

local HttpService = game:GetService("HttpService")
local API_URL = "https://replit.com/@huyefootball170/Banana?mobileBridge=1&hideBottomBar=1&forceTheme=replitLight&errorTrace=8b622c99-152e-4121-9b5d-cc17b14f6124&mobileWebview=1&supportsUpsellBridge=1&permissionRequest=1&speechRecognition=1&workspace2=1&storeReviewPrompt=1#whitelist.json" -- ğŸ”¹ Link API Replit

-- ğŸ“Œ HÃ m láº¥y key tá»« API JSON
local function getKeyFromAPI()
    local success, response = pcall(function()
        return game:HttpGet(API_URL, true) -- ğŸ”¹ ThÃªm `true` Ä‘á»ƒ trÃ¡nh lá»—i SSL
    end)

    if success then
        local data = HttpService:JSONDecode(response)
        if data and data.key then
            return data.key -- Láº¥y key tá»« JSON
        end
    end
    return nil
end

-- ğŸ”¹ Nháº­n key tá»« API vÃ  cháº¡y script
local key = getKeyFromAPI()
if key then
    print("âœ… Key nháº­n Ä‘Æ°á»£c tá»« API: " .. key)
    getgenv().Key = key
    loadstring(game:HttpGet("https://raw.githubusercontent.com/shinichidz/KaitunFruit/refs/heads/main/ShinichiHub"))()
else
    print("âŒ KhÃ´ng láº¥y Ä‘Æ°á»£c key! Vui lÃ²ng kiá»ƒm tra API.")
end
